<section>
<%if(success && success.length){%>

    <div class="alert alert-primary alert-dismissible fade show text-center" role="alert" style="margin: 0;">
      <strong><%=success%></strong> 
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  
<%}%>


<% if(error && error.length){ %>
  
  <div class="alert alert-danger alert-dismissible fade show text-center" role="alert" style="margin: 0;">
    <strong><%=error%></strong> 
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>






  <% } %> 

</section>